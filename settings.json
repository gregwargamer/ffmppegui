{
    "concurrency": {
        "global_jobs": 4,
        "video_jobs": 2
    },
    "ui": {
        "refresh_interval": 5,
        "show_server_details": true,
        "auto_select_best_server": true,
        "progress_refresh_interval": 2,
        "default_resolution": null
    },
    "distributed": {
        "auto_connect_servers": [
            {
                "ip": "192.168.1.80",
                "port": 8765
            }
