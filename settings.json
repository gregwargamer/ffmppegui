        "default_timeout": 30,
        "max_concurrent_jobs": 10,
        "preferred_encoders": [
            "h264_nvenc",
            "libx264"
        ]
    },
    "filename_template": {
        "template": "{nom_source}-{resolution}.{container_ext}"
    },
    "encoding_defaults": {
        "default_video_encoder": "libx264",
        "default_audio_encoder": "aac",
        "default_image_encoder": "png",
        "custom_flags": ""
    },
    "keep_folder_structure": true,
    "presets": {},
    "auto_connect_servers": []
